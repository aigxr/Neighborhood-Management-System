<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="9999" author="igor">

        <addForeignKeyConstraint
                baseTableName="file"
                baseColumnNames="tenant_id"
                constraintName="fk_file_tenant"
                referencedTableName="tenant"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="flat"
                baseColumnNames="owner_id"
                constraintName="fk_flat_owner_id"
                referencedTableName="owner"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="flat"
                baseColumnNames="block_id"
                constraintName="fk_flat_block_id"
                referencedTableName="block"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="flat"
                baseColumnNames="room_id"
                constraintName="fk_flat_room_id"
                referencedTableName="room"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="person"
                baseColumnNames="rental_id"
                constraintName="fk_person_rental_id"
                referencedTableName="rental"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="rental"
                baseColumnNames="block_id"
                constraintName="fk_rental_block_id"
                referencedTableName="block"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="rental"
                baseColumnNames="flat_id"
                constraintName="fk_rental_flat_id"
                referencedTableName="flat"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="rental"
                baseColumnNames="parking_id"
                constraintName="fk_rental_parking_id"
                referencedTableName="parking"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="block"
                baseColumnNames="neighborhood_id"
                constraintName="fk_block_neighborhood_id"
                referencedTableName="neighborhood"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="neighborhood"
                baseColumnNames="developer"
                constraintName="fk_neighborhood_developer_id"
                referencedTableName="developer"
                referencedColumnNames="id"/>

        <addForeignKeyConstraint
                baseTableName="parking"
                baseColumnNames="room_id"
                constraintName="fk_parking_room_id"
                referencedTableName="room"
                referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>